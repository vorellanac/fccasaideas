<p id="notice"><%= notice %></p>
<!-- navbar -->
<%= render 'layouts/navbar' %>

<h1>Ficha corta</h1>

<!-- custimizing -->
<table width="100%" border="0" cellpadding="0" cellspacing="0" bordercolor="#000000">
  <thead>
    <tr>
      <th>SKU</th>
      <th>Feria ID</th>
      <th>Fob</th>
      <th>Preciosug</th>
      <th>Productominmb</th>
      <th>Prvregcod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fichacorta.each do |fichacortum| %>
      <tr>
        <td><%= fichacortum.productomatnr %></td>
        <td><%= fichacortum.ferid %></td>
        <td><%= fichacortum.fob %></td>
        <td><%= fichacortum.preciosug %></td>
        <td><%= fichacortum.productominmb %></td>
        <td><%= fichacortum.prvregcod %></td>
        <td><%= link_to 'Mostrar', fichacortum %></td>
        <td><%= link_to 'Editar', edit_fichacortum_path(fichacortum) %></td>
        <td><%= link_to 'Destruir', fichacortum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo material', new_fichacortum_path %>
